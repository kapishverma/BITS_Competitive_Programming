
class Solution
{
    
    ArrayList<Integer> rotate(int N, int D)
    {
        ArrayList<Integer> op = new ArrayList<>();
        D=D%16;
        int a,b;
        a = ((N<<D)|(N>>(16-D)))&0xFFFF; //(0xFFFF means 1111 1111 1111 1111) & nikala ja raha 
        b = ((N>>D)|(N<<(16-D)))&65535;  // its decimal value is 65535
        op.add(a);
        op.add(b);
        return op;
    }
}
